{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"lib": [
			"dom",
			"dom.iterable",
			"es2015",
			"es2016",
			"es2017",
			"es2018",
			"es2019",
			"esnext"
		],
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": false,
		"sourceMap": false,
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		},
		"incremental": true
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "project.js"],
	"exclude": ["node_modules"]
}
